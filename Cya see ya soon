local _0x22 = 0

_0x20:AddTextbox({
    Name = "Enter Points",
    Default = "",
    TextDisappear = true,
    Callback = function(_0x23)
        _0x22 = tonumber(_0x23)
        print("Points entered:", _0x22)
    end
})

_0x20:AddButton({Name = "Set Points", Callback = function()
    if _0x22 and _0x22 > 0 then
        local _0x24 = {
            [1] = _0x22
        }
        game:GetService("ReplicatedStorage"):WaitForChild("BonusFolderFX"):WaitForChild("testingstats"):FireServer(unpack(_0x24))
        print("Points set to:", _0x22)
    else
        warn("Invalid points value")
    end
end})

