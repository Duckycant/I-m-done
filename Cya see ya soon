-- Infinite Health
local _0x6 = false
local _0x7
local _0x8

local function _0x9()
    local _0xA = game.Players.LocalPlayer
    local _0xB = _0xA.Character
    if _0xB then
        local _0xC = _0xB:FindFirstChildOfClass("Humanoid")
        if _0xC then
            _0xC.Health = math.huge
            _0xC.MaxHealth = math.huge

            if _0x7 then
                _0x7:Disconnect()
            end

            _0x7 = _0xC:GetPropertyChangedSignal("Health"):Connect(function()
                if _0x6 then
                    _0xC.Health = math.huge
                end
            end)

            _0xC:GetPropertyChangedSignal("MaxHealth"):Connect(function()
                if _0x6 then
                    _0xC.MaxHealth = math.huge
                end
            end)
        end
    end
end

_0x4:AddToggle({
    Name = "Infinite Health",
    Default = false,
    Callback = function(_0xD)
        _0x6 = _0xD

        if _0x6 then
            _0x9()

            if _0x8 then
                _0x8:Disconnect()
            end
            _0x8 = game.Players.LocalPlayer.CharacterAdded:Connect(_0x9)
        else
            if _0x7 then
                _0x7:Disconnect()
                _0x7 = nil
            end
            if _0x8 then
                _0x8:Disconnect()
                _0x8 = nil
            end

            local _0xA = game.Players.LocalPlayer
            local _0xB = _0xA.Character
            if _0xB then
                local _0xC = _0xB:FindFirstChildOfClass("Humanoid")
                if _0xC then
                    _0xC.Health = _0xC.MaxHealth
                end
            end
        end
    end
})

